#include <stdio.h>

int main ( )
{
  int n,m,i,j,gyo[10][10];
  int count=1;

  scanf("%d%d",n,m);

  for(i=0; i<n; i++){
    for(j=0; j<m; j++){
      gyo[j][i]=count;
      count++;
    }
  }

    for (i=0; i<n ; i++){
      for (j=0; j<m ; j++){
      printf(" %2d ",gyo[j][i]);
    }
    printf("\n");
  }
  return 0;
}
